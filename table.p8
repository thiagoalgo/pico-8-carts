function inc_y(y)
	return y + 8
end

function tab(lvl)
	return lvl * 10
end

function print_table(obj, y, lvl)	
	for key, value in pairs(obj) do
		if type(value) == "table" then
			print("table: " .. key, tab(lvl), y, 3)
			y = inc_y(y)
			y = print_table(value, y, lvl + 1)
			print("end talbe: " .. key, tab(lvl), y, 3)
			y = inc_y(y)
		else
			print(key .. ": " .. tostr(value), tab(lvl), y, 12)
			y = inc_y(y)
		end
	end
	return y
end

